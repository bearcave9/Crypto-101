#!/usr/bin/python

import sys
import csv

N = raw_input("Please enter N: ")
b = open('out_data.csv', 'w')
a = csv.writer(b)
data=[]
with open(sys.argv[1], 'r') as raw:
            
    for i in range(int(N)+1):
        line  = raw.next().strip()
        data.append(line)
        print line
    

a.writerows(data)
raw.close()            








